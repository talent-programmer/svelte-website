{
  "name": "gitpod-website",
  "description": "Gitpod.io Website",
  "version": "2.0.0",
  "private": true,
  "keywords": [
    "svelte"
  ],
  "scripts": {
    "watch:css": "postcss src/assets/global.pcss -o static/global.css -w",
    "watch:dev": "sapper dev --ext '.md .svelte'",
    "clean": "rm -fr ./__sapper__",
    "dev": "run-p watch:*",
    "copyDocsImages": "cp ./",
    "build": "run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.pcss -o static/global.css",
    "build:sapper": "sapper build --ext '.md .svelte'",
    "build:export": "sapper export --ext '.md .svelte'",
    "export": "run-s clean build:css build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "postinstall": "run-p postinstall:*",
    "postinstall:patchRemarkCopyLinkedFiles": "sed -i 's/cwd, path/cwd, filename: path/g' ./node_modules/remark-copy-linked-files/index.js"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.4.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "cssnano": "^4.1.10",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mdsvex": "^0.8.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "remark-copy-linked-files": "^1.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "sass": "^1.32.8",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-preprocess": "^4.6.9",
    "tailwindcss": "^2.0.4",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,svelte}": "prettier --write"
  }
}
